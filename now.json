{
  "name": "CourseCamp",
  "alias": [
    "teamcoursecamp.com",
    "www.teamcoursecamp.com"
  ],
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "package.json", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.js" },
    { "src": "/teach", "dest": "/earnMoneyTeaching.js" },
    { "src": "/courses/view/(.*)", "dest": "/courses/viewCourse.js?courseId=$1" }
  ]
}